{
  "rewrites": [
    {
      "source": "/*",
      "destination": "https://cxspigot.serveo.net/index.php*"
    },
    {
      "source": "/resources/",
      "destination": "https://cxspigot.serveo.net/resources/"
    },
    {
      "source": "/styles/",
      "destination": "https://cxspigot.serveo.net/styles/"
    },
    {
      "source": "/js/",
      "destination": "https://cxspigot.serveo.net/js/"
    },
    {
      "source": "/approval-queue/",
      "destination": "https://cxspigot.serveo.net/approval-queue/"
    },
    {
      "source": "/css.php",
      "destination": "https://cxspigot.serveo.net/css.php"
    },
    {
      "source": "/reports/",
      "destination": "https://cxspigot.serveo.net/reports/"
    },
    {
      "source": "/admin.php",
      "destination": "https://cxspigot.serveo.net/admin.php"
    }
  ]
}
